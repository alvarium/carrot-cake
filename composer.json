{
    "name": "alvarium/carrot-cake",
    "description": "RabbitMQ plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.5",
        "php-amqplib/php-amqplib": "^2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Alvarium\\CarrotCake\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alvarium\\CarrotCake\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
